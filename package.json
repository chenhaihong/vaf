{
  "name": "@erye/vaf",
  "version": "0.0.3",
  "description": "Vue Admin Framework.",
  "author": "erye <erye@tiiit.cn>",
  "scripts": {
    "dev": "vite",
    "dev:lib": "vite --mode development:lib",
    "build": "rimraf ./dist/* && vite build && vite build --mode build:development",
    "pub": "npm publish --access public --registry https://registry.npmjs.org/",
    "prepub": "npm run build"
  },
  "main": "./dist/vaf.umd.js",
  "module": "./dist/vaf.es.js",
  "style": "./dist/index.css",
  "exports": {
    ".": {
      "import": "./dist/vaf.es.js",
      "require": "./dist/vaf.umd.js"
    }
  },
  "license": "MIT",
  "keywords": [
    "vue admin",
    "Vue Admin Framework",
    "Admin Framework",
    "vaf",
    "vite"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chenhaihong/vaf.git"
  },
  "bugs": {
    "url": "https://github.com/chenhaihong/vaf/issues"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "axios": "^0.27.2",
    "element-plus": "^2.2.6",
    "nprogress": "^0.2.0",
    "rimraf": "^3.0.2",
    "sass": "^1.52.1",
    "vite": "^2.9.9",
    "vue": "^3.2.35",
    "vue-router": "^4.0.15",
    "vuex": "^4.0.2",
    "vuex-persistedstate": "^4.1.0"
  },
  "peerDependencies": {
    "axios": "^0.27.2",
    "element-plus": "^2.2.1",
    "nprogress": "^0.2.0",
    "vue": "^3.2.35",
    "vue-router": "^4.0.15",
    "vuex": "^4.0.2",
    "vuex-persistedstate": "^4.1.0"
  }
}
