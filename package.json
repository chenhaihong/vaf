{
  "name": "@erye/vaf",
  "version": "0.0.15",
  "description": "Vue Admin Framework.",
  "author": "erye <erye@tiiit.cn>",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:lib": "vite serve --mode development:lib",
    "build": "rimraf ./dist/* && vite build && vite build --mode production:minified",
    "preview": "vite preview",
    "pub": "npm publish --access public --registry https://registry.npmjs.org/",
    "predev:lib": "npm run build",
    "prepreview": "npm run build && vite build --mode preview",
    "prepub": "npm run build"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/vaf.umd.js",
  "module": "./dist/vaf.es.js",
  "style": "./dist/index.css",
  "license": "MIT",
  "keywords": [
    "vue admin",
    "Vue Admin Framework",
    "Admin Framework",
    "vaf",
    "vite"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chenhaihong/vaf.git"
  },
  "bugs": {
    "url": "https://github.com/chenhaihong/vaf/issues"
  },
  "devDependencies": {
    "@element-plus/icons-vue": "^2.0.9",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "axios": "^0.27.2",
    "element-plus": "^2.2.13",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.19",
    "rimraf": "^3.0.2",
    "sass": "^1.54.0",
    "vite": "^3.0.8",
    "vue": "^3.2.37",
    "vue-router": "^4.1.3"
  },
  "peerDependencies": {
    "@element-plus/icons-vue": "^2.0.9",
    "axios": "^0.27.2",
    "element-plus": "^2.2.13",
    "nprogress": "^0.2.0",
